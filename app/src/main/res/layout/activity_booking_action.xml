<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BookingAction">

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start Time"
        app:layout_constraintBottom_toTopOf="@+id/starttimeaction"
        app:layout_constraintStart_toStartOf="@+id/textView29"
        app:layout_constraintTop_toTopOf="@+id/starttimeaction" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="184dp"
        android:text="Status"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Testing Site"
        app:layout_constraintBaseline_toBaselineOf="@+id/testingsiteaction"
        app:layout_constraintStart_toStartOf="@+id/textView28" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Updated At"
        app:layout_constraintBaseline_toBaselineOf="@+id/updatedataction"
        app:layout_constraintStart_toStartOf="@+id/textView27" />

    <TextView
        android:id="@+id/statusaction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="37dp"
        app:layout_constraintBottom_toTopOf="@+id/testingsiteaction"
        app:layout_constraintStart_toStartOf="@+id/testingsiteaction" />

    <TextView
        android:id="@+id/testingsiteaction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="37dp"
        app:layout_constraintStart_toEndOf="@+id/textView29"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <TextView
        android:id="@+id/starttimeaction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="21dp"
        app:layout_constraintBottom_toTopOf="@+id/updatedataction"
        app:layout_constraintStart_toStartOf="@+id/updatedataction" />

    <TextView
        android:id="@+id/updatedataction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="68dp"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/testingsiteaction" />

    <Button
        android:id="@+id/modactionbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="Modify"
        app:layout_constraintBaseline_toBaselineOf="@+id/cancelbuttonaction"
        app:layout_constraintEnd_toStartOf="@+id/cancelbuttonaction"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/cancelbuttonaction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="213dp"
        android:text="cancel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/modactionbutton" />

    <Button
        android:id="@+id/undobutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginTop="54dp"
        android:text="UNDO"
        app:layout_constraintStart_toEndOf="@+id/modactionbutton"
        app:layout_constraintTop_toBottomOf="@+id/cancelbuttonaction" />
</androidx.constraintlayout.widget.ConstraintLayout>